{
    "name": "Youtube Downloader",
    "version": "1.0",
    "manifest_version": 2, 
    "content_scripts": [
        {
            "run_at" : "document_end",
            "matches": [ "<all_urls>"], 
            "js": ["content.js"]
        }
    ],
    "browser_action" : { 
        "default_popup":"popup.html",
        "default.title":  "Downloader"
    }
}